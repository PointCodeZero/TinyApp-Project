<!DOCTYPE html>
<html lang="en">
  <head>
    <title>TinyApp</title>
  </head>
  <body>
    <% include ./partials/_header %>
    <ul>
      <% for(var url in urls) { %>
        <li><%= url %>: <%= urls[url] %></li>
        <a href="/urls/<%= url %>">Edit</a>
        <form action="/urls/<%= url %>/delete" method="POST">
          <input type="submit" value="Delete">
        </form>
      <% }; %>
    </ul>
    <a href="/urls/new">Shortening</a>
  </body>
</html>